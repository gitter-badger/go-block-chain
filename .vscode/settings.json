{
	"editor.tabSize": 4,
	"go.lintFlags": [
        "--exclude=\"\bexported \\w+ (\\S*['.]*)([a-zA-Z'.*]*) should have comment or be unexported\b\""
    ],
    "go.formatTool": "gofmt",
    "go.coverOnSave": true,
    "go.liveErrors": {
        "enabled": true,
        "delay": 1000
    },
    "go.enableCodeLens": {
        "references": false,
        "runtest": true
    },
    "go.goroot": "/usr/local/go/",
    "go.playground": {
        "openbrowser": true,
        "share": true,
        "run": true
    },
    "go.autocompleteUnimportedPackages": true,
    "go.testTimeout": "120s",
    "go.vetOnSave": "off",
    // "go.gopath": "/usr/local/go/bin/go",
}